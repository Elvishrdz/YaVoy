            app:popUpTo="@id/testingFragment"
            app:popUpToInclusive="false" />
    </fragment>
    <fragment
        android:id="@+id/testingFragment"
        android:name="com.eahm.testing.TestingFragment"
        android:label="TestingFragment" />
</navigation>